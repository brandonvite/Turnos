@model Turnos.Models.Paciente

<h5>Paciente</h5>
<hr />
<div class="row">
    <div class="col s12 m6">
        <div class="card hoverable green lighten-1">
            <div class="card-content white-text">
                <span class="card-title">
                    <h5><b>@Html.DisplayFor(modelo => modelo.Nombre) @Html.DisplayFor(modelo => modelo.Apellido)</b>
                    </h5>
                </span>
                <dt>
                    <h6>@Html.DisplayNameFor(modelo => modelo.Direccion): @Html.DisplayFor(modelo =>
                        modelo.Direccion)</h6>
                </dt>
                <hr />
                <dt>
                    <h6> @Html.DisplayNameFor(modelo => modelo.Telefono): @Html.DisplayFor(modelo =>
                        modelo.Telefono)</h6>
                </dt>
                <hr />
                <dt>
                    <h6> @Html.DisplayNameFor(modelo => modelo.Email): @Html.DisplayFor(modelo => modelo.Email)</h6>
                </dt>
            </div>
            <div class="card-action blue-grey darken-2">
                <form asp-action="Edit">
                    <input type="hidden" asp-for="IdPaciente" />
                    <a asp-action="Edit" asp-route-id="@Model.IdPaciente">Editar</a>
                    <a asp-action="Index">Volver</a>
                </form>
            </div>
        </div>
    </div>
</div>